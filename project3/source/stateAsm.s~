	.data
	.word 1
jp:	
	.word default		;
	.word case1		;
	.word case2		;
	.word case3		;
	.word case4		;
	.word case5		;
	.word case6		;
	.word case7		;
	.word case8		;
	.word case9		;
	.word case10		;
	.word case11		;
	.word case12		;
	.word case13		;
	.word case14		;
	.word case15		;
	.word case16		;
	.word case17		;
	.word case18		;

	.text
.global f
	Mov &s, r12
	cmp #18,r2		;
	jnc default		;
	add r12,r12		;
	mov JT(r12),r0
case1:
	call buzzer_set_period(7124) ;
	goto switchDone
case2:
	call buzzer_set_period(7124) ;
	goto switchDone
case3:
	call buzzer_set_period(4992)	;
	goto switchDone
case4:
	call buzzer_set_period(4992) ;
	goto switchDone
case5:
	call buzzer_set_period(4545)	;
	goto switchDone
case6:
	call buzzer_set_period(4545) ;
	goto switchDone
case7:
	call buzzer_set_period(4992) ;
	goto switchDone
case8:
	call buzzer_set_period(5714) ;
	goto switchDone
case9:
	call buzzer_set_period(5714)	;
	goto switchDone
case10:
	call buzzer_set_period(5664) ;
	goto switchDone
case11:
	call buzzer_set_period(5664) ;
	goto switchDone
case12:
	call buzzer_set_period(6660)	;
	goto switchDone
case13:
	call buzzer_set_period(6660) ;
	goto switchDone
case14:
	call buzzer_set_period(7126)	;
	goto switchDone
case15:
	call buzzer_set_period(7126) ;
	goto switchDone
switchDone:
	Pop r0

	
global light
light:
	cmp r12,0		;
	JNZ off
	call turn_on()
off:
	call turn_off()
